{
  "name": "wishlist-app",
  "version": "1.0.1",
  "description": "Full Stack React App with Authentication",
  "main": "server/server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server/server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd server/client && npm run start",
    "seed": "node server/scripts/seedDB.js",
    "install": "cd server/client && yarn install",
    "build": "cd server/client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "author": "Team 6 (Jon, Charlie, Mike , Sundar)",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^3.5.0",
    "nodemon": "^1.11.0"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bcrypt": "^3.0.2",
    "bcryptjs": "^2.4.3",
    "connect-mongo": "^2.0.1",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "gh-pages": "^2.0.1",
    "if-env": "^1.0.4",
    "moment": "^2.22.2",
    "mongoose": "^5.3.4",
    "morgan": "^1.9.1",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "postscribe": "^2.0.8",
    "quagga": "^0.12.1",
    "react-bootstrap": "^0.32.4",
    "react-materialize": "^2.4.7",
    "react-moment": "^0.8.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ocskier/Proj3-Team6.git"
  },
  "bugs": {
    "url": "https://github.com/ocskier/Proj3-Team6/issues"
  },
  "homepage": "https://github.com/ocskier/Proj3-Team6#readme"
}
